using System;
using System.Collections.Generic;
using System.Drawing;
using System.Text;

namespace Maticsoft.BLL.ScanPortImage.imageStructure.imageDataBlock
{
    /// <summary>
    /// 竞彩篮球大小分赛事数据块
    /// </summary>
   public class JLDXFDataBlock : ImageDataBlock
    {
       public JLDXFDataBlock(int startx, int starty)
       {
            this.startPoint = new System.Drawing.Point(startx, starty);
        }

        /// <summary>
        /// 根据传入的数据，得到需要描的几个点的起始坐标
        /// </summary>
        /// <param name="data"></param>
        /// <returns></returns>
        public override List<Point> getPointArrayByData(String data)
        {
            List<Point> plist = new List<Point>();
            //取对阵的点
            this.getSMGRacePoints(plist,"",this.startPoint);//对阵的点
            //取竞猜结果的点
           return plist;
        }
    }
}
